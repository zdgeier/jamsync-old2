{
    "protoc": {
        "path": "/opt/homebrew/bin/protoc",
        "compile_on_save": false,
        "options": [
            "--proto_path=proto",
            "--go_out=gen/pb",
            "--go_opt=paths=source_relative",
            "--go-grpc_out=gen/pb",
            "--go-grpc_opt=paths=source_relative",
        ]
    }
}